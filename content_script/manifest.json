{
  "manifest_version": 2,

  "name": "Exemplo 3 - Mudando cor de fundo da pagina",
  "version": "1.0",
  "description": "Usando content scripts",
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["script.js"]
  }],
  "permissions": [
    "tabs", "http://*/*", "http://*/*"
  ]
}
